<?xml version="1.0" encoding="UTF-8"?>
<project name="decora-sse" default="all" basedir=".">

  <target name="compile-native">
    <exec executable="make" dir="../decora-sse-native" failonerror="true">
      <env key="JDK_HOME" value="${platform.home}"/>
      <arg value="all"/>
    </exec>
  </target>

  <target name="clean-native">
    <exec executable="make" dir="../decora-sse-native" failonerror="true">
      <arg value="clean"/>
    </exec>
  </target>

</project>
