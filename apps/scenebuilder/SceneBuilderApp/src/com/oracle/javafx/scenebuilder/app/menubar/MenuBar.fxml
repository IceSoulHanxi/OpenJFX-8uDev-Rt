<items><MenuItem disable="true" mnemonicParsing="false" text="%scenestylesheet.none" />
</items></Menu>
                <Menu fx:id="openSceneStyleSheetMenu" mnemonicParsing="false" text="%menu.title.open.stylesheet">
<items><MenuItem disable="true" mnemonicParsing="false" text="%scenestylesheet.none" />
</items></Menu>
              </items>
